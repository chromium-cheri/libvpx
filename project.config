[access]
	inheritFrom = webm
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[project]
	state = active
[access "refs/*"]
	owner = group Administrators
[access "refs/heads/experimental"]
	forgeAuthor = group webm-admins
	forgeCommitter = group webm-admins
	push = group webm-admins
[access "refs/heads/test"]
	exclusiveGroupPermissions = read
	read = group webm-releasers
