[access]
	inheritFrom = webm
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
	action = rebase if necessary
[project]
[access "refs/*"]
	forgeAuthor = group webm-admins
	push = +force group webm-admins
	pushMerge = group webm-admins
	createTag = group webm-admins
	create = group webm-admins
	createSignedTag = group webm-admins
[access "refs/for/refs/heads/nextgenv2"]
	exclusiveGroupPermissions = push
	push = deny group Logged-in Public Users
	push = deny group webm-committers
[access "refs/for/refs/heads/experimental"]
	exclusiveGroupPermissions = push
	push = deny group Logged-in Public Users
	push = deny group webm-committers
[access "refs/for/refs/heads/vp9-preview"]
	exclusiveGroupPermissions = push
	push = deny group Logged-in Public Users
	push = deny group webm-committers
[access "refs/heads/*"]
	create = group webm-releasers
	addPatchSet = group open-codecs-scoped-luci-account
	read = group open-codecs-scoped-luci-account
	submit = group open-codecs-scoped-luci-account
	labelAs-Commit-Queue = +0..+2 group open-codecs-scoped-luci-account
[access "refs/heads/nextgenv2"]
	exclusiveGroupPermissions = submit
	submit = deny group Logged-in Public Users
	submit = deny group webm-committers
[label "Commit-Queue"]
	function = NoBlock
	defaultValue = 0
	copyCondition = changekind:NO_CHANGE
[access "refs/heads/master"]
	exclusiveGroupPermissions = push submit
[plugin "zosscoverage"]
	enable = false
	enableLowCoverageAlerting = false
	enableVoting = false
	incrementalCoverageDisplayErrorBelow = 0
	incrementalCoverageDisplayWarningBelow = 0
	labelName = Code-Coverage
	minAffectedFilesToEnforce = 0
	minAffectedLinesToEnforce = 0
	negativeVoteValue = -1
	positiveVoteValue = 1
[submit-requirement "Commit-Queue"]
	description = Submit requirement for the 'Commit-Queue' label
	applicableIf = is:false
	submittableIf = is:true
	canOverrideInChildProjects = true
